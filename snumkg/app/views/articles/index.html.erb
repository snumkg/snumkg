
<div id="article_wrapper">
  <table class="table">
    <% for article in @articles %>
      <tr>
        <td><%= article.title %> <div class="right"> 작성자: <%= article.user.username %></div></td>
      </tr>
      <tr>
        <td><%= article.body %></td>
      </tr>
    <% end %>
  </table>

  <%= link_to "글쓰기", new_article_path(:tab_name => params[:tab_name], :board_name => params[:board_name]) %>
</div>
