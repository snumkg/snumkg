<!DOCTYPE html>
<html>
<head>
  <title>Snumkg</title>
  <%= stylesheet_link_tag    "home", controller_path, :media => "all" %>
  <%= javascript_include_tag "jquery", "jquery_ujs" %>
  <%= javascript_include_tag "home", controller_path %>
  <%= javascript_include_tag "bootstrap-modal" %>
  <%= javascript_include_tag "signup" %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body>
  <div id="wrapper">
    <%= render 'shared/error_messages' %>
    <%= render 'layouts/success_message' %>
    <div id="content">
      <%= yield %>
    </div>

    <%= render :partial => 'shared/signup', :locals => {:@user => @new_user} %>
  </div>
</body>
</html>
